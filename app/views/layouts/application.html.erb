<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>House</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag :application, media: :all %>
  </head>
  <body>
    <%= yield %>

    <%= javascript_tag do %>
      var DB = {};
      DB.households = <%= households_json %>;
      DB.people = <%= people_json %>;
    <% end %>

    <%= javascript_include_tag 'vendor.bundle' %>
    <%= javascript_include_tag 'bundle' %>
  </body>
</html>
